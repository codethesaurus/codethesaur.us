    "insert_into_middle_of_ordered_mutable_list": {
      "name": "Insert element in middle",
      "code": "array.splice(2, 0, 8)",
      "comment": "Here, 2 is the index to add to, 0 is the number of elements to remove, 8 is the element to add"
    },
